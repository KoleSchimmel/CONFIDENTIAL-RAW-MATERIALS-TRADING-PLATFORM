{
  "name": "anonymous-delivery-network",
  "version": "1.0.0",
  "description": "Anonymous Delivery Network with FHE encryption",
  "main": "index.html",
  "scripts": {
    "compile": "cross-env npx hardhat compile",
    "deploy": "cross-env npx hardhat run deploy/deploy.ts --network hardhat",
    "deploy:zama": "cross-env npx hardhat run deploy/deploy.ts --network zama",
    "deploy:sepolia": "cross-env npx hardhat run deploy/deploy.ts --network sepolia",
    "test": "cross-env npx hardhat test",
    "test:watch": "cross-env npx hardhat test --watch",
    "test:coverage": "cross-env npx hardhat coverage",
    "test:gas": "cross-env REPORT_GAS=true npx hardhat test",
    "clean": "npx hardhat clean",
    "typechain": "npx hardhat typechain",
    "lint": "npx solhint 'contracts/**/*.sol'",
    "serve": "npx http-server . -p 3000 -c-1 --cors"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@typechain/ethers-v6": "^11.1.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "ethers": "^6.0.0",
    "hardhat": "^2.24.3",
    "hardhat-coverage": "^2.2.0",
    "hardhat-gas-reporter": "^1.0.0",
    "mocha": "^10.2.0",
    "solhint": "^4.0.0",
    "solidity-coverage": "^0.8.0",
    "typechain": "^8.3.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@fhevm/hardhat-plugin": "^0.0.1-3",
    "@fhevm/solidity": "^0.7.0"
  },
  "keywords": [
    "blockchain",
    "privacy",
    "delivery",
    "fhe",
    "anonymous"
  ],
  "author": "",
  "license": "MIT"
}
